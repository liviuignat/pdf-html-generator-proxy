{
  "name": "pdf-service-benchmarking",
  "version": "1.0.0",
  "description": "",
  "author": "Liviu Ignat",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "start": "node ./node_modules/better-npm-run start",
    "dev": "concurrent --kill-others \"npm run watch-lint\" \"npm run start-dev\"",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-lint": "watch 'npm run lint' ./src",
    "lint": "eslint -c .eslintrc ./src"
  },
  "betterScripts": {
    "start": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "watch 'npm run lint' ./src",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_ENV": "development",
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.10",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "piping": "^1.0.0-rc.3",
    "superagent": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "watch": "^0.19.1"
  }
}
